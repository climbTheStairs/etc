#!/bin/sh
set -o noclobber
export PATH=~/bin:$PATH
export PATH=~/bin/spec:$PATH # TODO: hopefully tmp, try think o sth better
export XDG_CACHE_HOME=~/cache
export XDG_CONFIG_HOME=~/etc
export XDG_DATA_HOME=~/sha
export XDG_STATE_HOME=~/state
. "$XDG_CONFIG_HOME/sh/vars"
. "$XDG_CONFIG_HOME/sh/alias"
"$XDG_CONFIG_HOME/sh/motd"
